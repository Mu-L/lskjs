    "vasync": "2.2.0",
    "verror": "1.10.0"
  },
  "// dtrace-provider": "~0.8 required for dtrace features",
  "// moment": "required for local time with CLI",
  "// mv": "required for RotatingFileStream",
  "nonOptionalDeps": {
    "dtrace-provider": "~0.8",
    "mv": "~2",
    "safe-json-stringify": "~1",
    "moment": "2.10.6"
  }
