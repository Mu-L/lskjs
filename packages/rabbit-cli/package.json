{
  "name": "@lskjs/rabbit-cli",
  "version": "2.53.0",
  "description": "CLI for quick post json's to RabbitMQ",
  "keywords": [
    "queue",
    "rabbit"
  ],
  "bin": {
    "lskrabbit": "./bin/cli.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/lskjs/lskjs/tree/master/packages/rabbit-cli",
  "license": "MIT",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "scripts": {
    "bootstrap": "lsk run bootstrap",
    "build": "lsk run build",
    "dev": "lsk run dev",
    "dev:server": "lsk run dev:server",
    "i18": "lsk run i18",
    "prepublishOnly": "lsk run prepublishOnly",
    "release": "lsk run release",
    "release:yes": "lsk run release:yes",
