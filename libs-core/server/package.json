{
  "name": "@lskjs/server",
  "version": "2.85.1",
  "description": "LSK server.",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/env": "2.85.1",
    "@lskjs/err": "2.85.1",
    "@lskjs/module": "2.85.1",
    "@lskjs/utils": "2.72.0"
  },
  "devDependencies": {
    "@lskjs/db": "2.85.1",
    "@lskjs/i18": "2.85.1",
    "@lskjs/webserver": "2.76.3",
    "axios": "0.21.2",
    "bluebird": "3.7.2",
    "lodash": "4.17.21",
    "supertest": "6.1.5"
  },
  "peerDependencies": {
    "@lskjs/db": "2.x",
    "@lskjs/i18": "2.x",
    "@lskjs/webserver": "2.x",
    "axios": "0.x",
    "bluebird": "3.7.x",
    "lodash": "4.17.x"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config/nostrict"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master/packages/server",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [],
  "////": "/////////========================/////////========================/////////========================/////////"
}
