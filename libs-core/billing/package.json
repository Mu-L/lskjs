{
  "name": "@lskjs/billing",
  "version": "3.16.4",
  "description": "LSK.js module for adding billing in cabinet",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/err": "2.85.1",
    "@lskjs/module": "2.85.1",
    "@lskjs/utils": "2.72.0"
  },
  "devDependencies": {
    "@lskjs/db": "^3.16.4",
    "@lskjs/server-api": "^3.16.4",
    "axios": "0.21.2",
    "bluebird": "3.7.2",
    "crypto-js": "4.1.1",
    "lodash": "4.17.21",
    "yandex-checkout": "1.0.13"
  },
  "peerDependencies": {
    "@lskjs/db": "2.x",
    "@lskjs/server-api": "2.x",
    "axios": "0.x",
    "bluebird": "3.7.x",
    "crypto-js": "4.x",
    "lodash": "4.17.x"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config/nostrict"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-core/billing",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs-core/billing",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "billing",
    "bitpay",
    "coingate",
    "ecommpay",
    "epayments",
    "g2a",
    "paypal",
    "stripe",
    "interkassa",
    "yandexCheckout"
  ],
  "////": "/////////========================/////////========================/////////========================/////////",
  "optionalDeps": {
    "yandex-checkout": "*",
    "coingate-v2": "*"
  }
}
