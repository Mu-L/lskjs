{
  "name": "@lskjs/webserver",
  "version": "3.1.1",
  "description": "LSK module webserver.",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/env": "2.85.1",
    "@lskjs/err": "2.85.1",
    "@lskjs/log": "2.85.1",
    "@lskjs/module": "2.85.1",
    "@lskjs/utils": "2.72.0",
    "accept-language-parser": "1.5.0",
    "body-parser": "1.19.0",
    "cacheman": "2.2.1",
    "connect-static-file": "2.0.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "emotion": "10.0.27",
    "emotion-theming": "10.0.27",
    "express": "4.17.1",
    "express-jwt": "6.1.0",
    "fast-safe-stringify": "2.1.1",
    "nanoid": "3.1.29",
    "portfinder": "1.0.28",
    "socket.io": "4.2.0",
    "socket.io-as-promised": "1.0.0",
    "socket.io-redis": "6.1.1"
  },
  "devDependencies": {
    "@lskjs/db": "^3.1.1",
    "@lskjs/i18": "^3.1.1",
    "axios": "0.21.2",
    "bluebird": "3.7.2",
    "lodash": "4.17.21",
    "supertest": "6.1.6"
  },
  "peerDependencies": {
    "@lskjs/db": "2.33.1",
    "@lskjs/i18": "2.33.1",
    "axios": "0.x",
    "bluebird": "3.7.x",
    "lodash": "4.17.x"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config/nostrict"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master/packages/webserver",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "webserver",
    "express"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
