{
  "name": "@lskjs/auth",
  "version": "2.85.1",
  "description": "LSK.js – auth – module for authorization by login and password and singup through social networks",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "contributors": [
    "Andrey Kondalov <andrey@kondalov.com> (https://github.com/yukioru)"
  ],
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/env": "2.85.1",
    "@lskjs/err": "2.85.1",
    "@lskjs/module": "2.85.1",
    "@lskjs/utils": "2.72.0",
    "bcryptjs": "2.4.3",
    "jsonwebtoken": "8.5.1",
    "passport": "0.4.1",
    "qs": "6.10.1",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "@lskjs/db": "2.85.1",
    "@lskjs/mobx": "2.13.1",
    "@lskjs/server-api": "2.85.1",
    "axios": "0.21.2",
    "bluebird": "3.7.2",
    "js-cookie": "3.0.0",
    "lodash": "4.17.21",
    "mobx": "5.15.4",
    "node-accountkit": "1.1.6",
    "passport-facebook": "3.0.0",
    "passport-telegram-official": "1.1.0",
    "passport-vkontakte": "0.5.0",
    "passport-youtube-v3": "2.1.0"
  },
  "peerDependencies": {
    "@lskjs/db": "2.x",
    "@lskjs/mobx": "2.x",
    "@lskjs/server-api": "2.x",
    "axios": "0.x",
    "bluebird": "3.7.x",
    "js-cookie": "3.x",
    "lodash": "4.17.x",
    "mobx": "<6"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config/nostrict"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master/packages/auth",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "auth",
    "oAuth",
    "passport",
    "facebook",
    "telegram",
    "vk",
    "vkontakte"
  ],
  "////": "/////////========================/////////========================/////////========================/////////",
  "optionalDeps": {
    "node-accountkit": "*",
    "passport-facebook": "*",
    "passport-telegram-official": "*",
    "passport-vkontakte": "*",
    "passport-youtube-v3": "*"
  }
}