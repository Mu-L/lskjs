{
  "name": "@lskjs/sms",
  "version": "3.17.0",
  "description": "LSK module for sms.",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/env": "2.85.1",
    "@lskjs/err": "2.85.1",
    "@lskjs/utils": "2.72.0"
  },
  "devDependencies": {
    "axios": "0.21.2",
    "plivo": "4.12.0"
  },
  "peerDependencies": {
    "axios": "0.x"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config/nostrict"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-core/sms",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs-core/sms",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "sms"
  ],
  "////": "/////////========================/////////========================/////////========================/////////",
  "optionalDeps": {
    "plivo": "*",
    "telesignsdk": "*"
  }
}
