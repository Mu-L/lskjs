{
  "name": "@lskjs/i18",
  "version": "3.8.1",
  "description": "LSK module for internationalization with i18next.",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/env": "2.85.1",
    "@lskjs/err": "2.85.1",
    "@lskjs/module": "2.85.1",
    "i18next": "19.1.0",
    "i18next-xhr-backend": "3.2.2"
  },
  "devDependencies": {
    "bluebird": "3.7.2",
    "js-cookie": "3.0.0",
    "mobx": "5.15.4"
  },
  "peerDependencies": {
    "bluebird": "3.7.x",
    "js-cookie": "3.x",
    "mobx": "5.x"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config/nostrict"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-core/i18",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs-core/i18",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "i18",
    "internationalization",
    "schema"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
