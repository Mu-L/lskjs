{
  "name": "@lskjs/kafka",
  "version": "3.1.1",
  "description": "LSK module for kafka.",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/err": "2.85.1",
    "@lskjs/module": "2.85.1",
    "@lskjs/utils": "2.72.0",
    "kafkajs": "2.1.0"
  },
  "devDependencies": {
    "bluebird": "3.7.2",
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "bluebird": "3.7.x",
    "lodash": "4.17.x"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config/nostrict"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master/packages/kafka",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "kafka"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
