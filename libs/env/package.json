{
  "name": "@lskjs/env",
  "version": "3.0.0-alpha.8",
  "description": "LSK.js – env – Scripts for environment detecton",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "scripts": {
    "build": "          lsk run build",
    "dev": "            lsk run dev",
    "release": "        lsk run release",
    "test": "           lsk run test",
    "test:watch": "     lsk run test:watch"
  },
  "dependencies": {
    "@lskjs/tty": "^3.0.0-alpha.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "repository": "https://github.com/lskjs/lskjs/tree/master/libs/env",
  "license": "MIT",
  "keywords": [
    "environment variables",
    "env"
  ],
  "//": "//",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1kb"
    }
  ]
}
