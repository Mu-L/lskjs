{
  "name": "@lskjs/env",
  "version": "3.0.0-alpha.24",
  "description": "LSK.js – env – Scripts for environment detecton",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "build": "          lsk run build",
    "dev": "            lsk run dev",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release",
    "test": "           lsk run test"
  },
  "dependencies": {
    "@lskjs/tty": "^3.0.0-alpha.24"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs/env",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "keywords": [
    "environment variables",
    "env"
  ],
  "//": "//",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1kb"
    }
  ]
}
