{
  "name": "@lskjs/colors",
  "version": "3.0.0-alpha.7",
  "description": "Yet another colors library â€“ like picocolors but with for browser too",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "scripts": {
    "build": "          lsk run build",
    "dev": "            lsk run dev",
    "release": "        lsk run release",
    "test": "           lsk run test",
    "test:watch": "     lsk run test:watch"
  },
  "dependencies": {
    "@lskjs/env": "3.0.0-alpha.4"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "node": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./*": {
      "default": "./lib/*.js",
      "require": "./lib/*.js",
      "import": "./lib/*.mjs",
      "node": "./lib/*.js",
      "types": "./lib/*.d.ts"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "repository": "https://github.com/lskjs/lskjs/tree/master/libs/log",
  "keywords": [
    "colors"
  ],
  "license": "MIT",
  "//": "//",
  "contributors": [
    "Vladislav Denisov (https://github.com/NewSoftwareCulture)"
  ],
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "clean-publish": {
    "packageManager": "pnpm"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "2.2kb"
    }
  ]
}
