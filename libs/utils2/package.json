{
  "name": "@lskjs/utils2",
  "version": "2.85.1",
  "description": "LSK.js – utils – helpers and functions",
  "keywords": [],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/lskjs/lskjs/tree/master/libs/utils2",
  "license": "MIT",
  "author": "Igor Suvorov <hi@isuvorov.com>",


  "_type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "node": "./lib/index.js"
    },
    "./*": {
      "default": "./lib/*.js",
      "require": "./lib/*.js",
      "import": "./lib/*.mjs",
      "node": "./lib/*.js"
    }
  },
  "scripts": {
    "dev": "            tsup --watch",
    "clean": "          rm -rf ./lib/* && rm -rf ./coverage && rm -rf ./package",
    "build": "          tsup",
    "test": "           npm run test:jest && npm run test:eslint",
    "test:watch": "     jest --watch",
    "test:jest": "      jest --coverage",
    "test:eslint": "    eslint src",
    "release": "        npm run clean && npm run build && npm run test && clean-publish"
  },

  "dependencies": {
    "fishbird": "^0.19.0"
  },
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "files": [
    "lib",
    "docker-stack.yml",
    "Dockerfile"
  ]
}
