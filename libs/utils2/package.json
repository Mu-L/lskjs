{
  "name": "@lskjs/utils2",
  "version": "3.0.0-alpha.20",
  "description": "LSK.js – utils – helpers and functions",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "scripts": {
    "build": "          lsk run build",
    "dev": "            lsk run dev",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release",
    "test": "           lsk run test"
  },
  "dependencies": {
    "@lskjs/lodash": "^3.0.0-alpha.20",
    "fishbird": "^0.19.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "repository": "https://github.com/lskjs/lskjs/tree/master/libs/utils2",
  "//": "//",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1.6kb"
    }
  ]
}
