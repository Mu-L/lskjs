{
  "name": "@lskjs/utils2",
  "version": "3.0.0-alpha.7",
  "description": "LSK.js – utils – helpers and functions",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "repository": "https://github.com/lskjs/lskjs/tree/master/libs/utils2",
  "dependencies": {
    "@lskjs/lodash": "^3.0.0-alpha.7",
    "fishbird": "^0.19.0"
  },
  "scripts": {
    "build": "          lsk run build",
    "dev": "            lsk run dev",
    "release": "        lsk run release",
    "test": "           lsk run test",
    "test:watch": "     lsk run test:watch"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "node": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./*": {
      "default": "./lib/*.js",
      "require": "./lib/*.js",
      "import": "./lib/*.mjs",
      "node": "./lib/*.js",
      "types": "./lib/*.d.ts"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1.5kb"
    }
  ]
}
