{
  "name": "@lskjs/lang",
  "version": "3.1.0-alpha.0",
  "description": "LSK.js – lang – text language detection",
  "author": "Nikita Savyolov <ga2mer@yandex.ru> (https://github.com/ga2mer)",
  "contributors": [
    "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)"
  ],
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@babel/runtime": "7.20.7",
    "@lskjs/algos": "^3.1.0-alpha.0",
    "@lskjs/module": "^3.1.0-alpha.0",
    "cld": "2.8.4",
    "fishbird": "^0.20.0",
    "languagedetect": "2.0.0"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master/libs/lang",
