{
  "name": "@lskjs/err",
  "version": "3.0.0-alpha.8",
  "description": "LSK.js – err – Error class and utils for custom code and message extracting",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "scripts": {
    "build": "          lsk run build",
    "dev": "            lsk run dev",
    "release": "        lsk run release",
    "test": "           lsk run test",
    "test:watch": "     lsk run test:watch"
  },
  "dependencies": {
    "@lskjs/algos": "^3.0.0-alpha.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "node": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./*": {
      "default": "./lib/*.js",
      "require": "./lib/*.js",
      "import": "./lib/*.mjs",
      "node": "./lib/*.js",
      "types": "./lib/*.d.ts"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "repository": "https://github.com/lskjs/lskjs/tree/master/libs/err",
  "license": "MIT",
  "keywords": [
    "errors wrapper",
    "errors",
    "error"
  ],
  "//": "//",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "4kb"
    }
  ]
}
