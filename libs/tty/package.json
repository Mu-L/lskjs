{
  "name": "@lskjs/tty",
  "version": "3.0.0-alpha.30",
  "description": "LSK.js – tty – TTY detection for Node.js and browser",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           echo skip",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "tty": "^1.0.1"
  },
  "//": "///////////========================/////////========================/////////========================//////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "browser": "./lib/client.js",
      "node": "./lib/server.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1kb"
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs/tty",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "tty detecton"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}